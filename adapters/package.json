{
  "name": "@chainlink/ea",
  "version": "0.0.1",
  "description": "External adapters",
  "main": "dist/adapters/index.js",
  "types": "dist/adapters/index.d.ts",
  "files": [
    "dist/adapters"
  ],
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "yarn build && yarn test",
    "setup": "yarn build",
    "build": "tsc -b",
    "lint": "eslint --ignore-path ../.eslintignore . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --ignore-path ../.eslintignore . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "echo \"Error: no test specified\"",
    "test:unit": "echo \"Error: no test specified\"",
    "test:integration": "echo \"Error: no test specified\"",
    "start:jsonrpc": "node -e 'require(\"./dist/adapters/index.js\").jsonrpc.handlers.server()'",
    "start:dnsquery": "node -e 'require(\"./dist/adapters/index.js\").dnsquery.handlers.server()'",
    "start:cryptoapis": "node -e 'require(\"./dist/adapters/index.js\").cryptoapis.handlers.server()'",
    "start": "node -e 'require(\"./dist/adapters/index.js\").'$npm_config_adapter'.handlers.server()'"
  },
  "dependencies": {
    "object-path": "^0.11.4",
    "@chainlink/ea-bootstrap": "^0.1.4",
    "@chainlink/ea-factories": "^0.0.1",
    "@chainlink/external-adapter": "^0.2.7"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
