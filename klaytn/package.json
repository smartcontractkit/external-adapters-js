{
  "name": "@chainlink/klaytn-adapter",
  "version": "0.0.1",
  "description": "Chainlink Klaytn adapter",
  "keywords": [
    "Klaytn",
    "Chainlink",
    "LINK",
    "blockchain",
    "oracle"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "setup": "yarn build",
    "build": "tsc -b",
    "lint": "eslint --ignore-path ../.eslintignore . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --ignore-path ../.eslintignore . --ext .js,.jsx,.ts,.tsx --fix",
    "server": "node -e 'require(\"./dist/index.js\").server()'",
    "server:dist": "node -e 'require(\"./dist/index.js\").server()'",
    "start": "yarn server:dist"
  },
  "license": "MIT",
  "dependencies": {
    "caver-js": "^1.5.5",
    "web3": "^1.2.9",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
