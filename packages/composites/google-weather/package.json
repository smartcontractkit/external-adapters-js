  "devDependencies": {
    "@chainlink/ea-test-helpers": "workspace:*",
    "@chainlink/types": "workspace:*",
    "@types/convert-units": "2.3.5",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.19",
    "typescript": "4.3.5"
  }
