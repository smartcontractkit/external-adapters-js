{
  "name": "@chainlink/anchor-adapter",
  "version": "4.0.16",
  "description": "Chainlink anchor adapter.",
  "keywords": [
    "Chainlink",
    "LINK",
    "BTC",
    "Bitcoin",
    "blockchain",
    "oracle",
    "anchor"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
