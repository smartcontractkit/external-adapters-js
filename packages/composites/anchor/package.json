    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/jest": "27.5.2",
    "@types/node": "16.11.68",
    "@types/supertest": "2.0.16",
    "nock": "13.5.4",
    "supertest": "6.2.4",
    "typescript": "5.0.4"
  }
}
