{
  "name": "@chainlink/crypto-volatility-index-adapter",
  "version": "0.0.1",
  "description": "The Crypto volatility index (CVI)",
  "keywords": [
    "Chainlink",
    "LINK",
    "COTI",
    "CVI",
    "blockchain",
    "oracle"
