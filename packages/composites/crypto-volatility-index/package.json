    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@chainlink/types": "0.0.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.35",
    "@types/sinon": "^9.0.11",
    "prettier": "^2.0.5",
    "sinon": "^10.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
