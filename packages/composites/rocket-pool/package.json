{
  "name": "@chainlink/rocket-pool-adapter",
  "version": "1.1.18",
  "description": "Chainlink external adapter for Rocket Pool price feeds.",
  "keywords": [
    "Chainlink",
    "LINK",
    "blockchain",
    "oracle",
    "rocket-pool",
    "RETH",
    "rETH",
    "rocket",
    "pool"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "url": "https://github.com/smartcontractkit/external-adapters-js",
    "type": "git"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist && rm -f tsconfig.tsbuildinfo",
    "prepack": "yarn build",
    "build": "tsc -b",
    "server": "node -e 'require(\"./index.js\").server()'",
    "server:dist": "node -e 'require(\"./dist/index.js\").server()'",
    "start": "yarn server:dist"
  },
  "dependencies": {
    "@chainlink/ea-bootstrap": "workspace:*",
    "@chainlink/ea-reference-data-reader": "workspace:*",
    "decimal.js": "^10.3.1",
    "ethers": "^5.4.6",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@chainlink/contracts": "^0.8.0",
    "@types/jest": "^29.5.14",
    "@types/node": "22.14.1",
    "@types/supertest": "2.0.16",
    "nock": "13.5.6",
    "supertest": "6.2.4",
    "typescript": "5.8.3"
  }
}
