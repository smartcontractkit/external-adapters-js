{
  "name": "@chainlink/medianizer-adapter",
  "version": "1.0.2",
  "description": "Chainlink medianizer adapter.",
  "keywords": [
    "Chainlink",
    "LINK",
    "BTC",
    "Bitcoin",
    "blockchain",
    "oracle",
    "medianizer"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
