    "start": "yarn server:dist"
  },
  "dependencies": {
    "@chainlink/ea-bootstrap": "workspace:*",
    "axios": "^0.21.1",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@chainlink/ea-test-helpers": "workspace:*",
    "@chainlink/types": "workspace:*",
    "@types/jest": "27.0.3",
    "@types/node": "14.17.34",
    "sinon": "11.1.2",
    "typescript": "4.5.2"
  }
}
