{
  "name": "@chainlink/synth-index-adapter",
  "version": "0.1.1",
  "description": "Chainlink Synth Index adapter",
  "keywords": [
    "Chainlink",
    "LINK",
    "blockchain",
    "oracle"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
