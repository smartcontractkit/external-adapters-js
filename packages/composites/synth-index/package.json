{
  "name": "@chainlink/synth-index-adapter",
  "version": "0.1.0",
  "description": "Chainlink Synth Index adapter",
  "keywords": [
    "Chainlink",
    "LINK",
    "blockchain",
    "oracle"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
