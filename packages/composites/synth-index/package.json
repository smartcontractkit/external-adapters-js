    "type-fest": "^1.2.1"
  },
  "devDependencies": {
    "@chainlink/types": "0.0.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.35",
    "@types/supertest": "^2.0.11",
    "nock": "^13.1.0",
    "supertest": "^6.1.3",
    "typescript": "^4.2.3"
