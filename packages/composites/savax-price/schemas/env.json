{
  "$id": "https://external-adapters.chainlinklabs.com/schemas/savax-price-adapter.json",
  "title": "Chainlink External Adapter for sAVAX price",
  "required": ["AVALANCHE_RPC_URL"],
  "type": "object",
  "properties": {
    "AVALANCHE_RPC_URL": {
      "type": "string"
    },
    "AVALANCHE_CHAIN_ID": {
      "required": false,
      "description": "The chain id to connect to",
      "type": "string",
      "default": "43114"
    },
    "SAVAX_ADDRESS": {
      "default": true,
      "type": "string"
    }
  },
  "allOf": [
    {
      "$ref": "https://external-adapters.chainlinklabs.com/schemas/ea-bootstrap.json"
    }
  ]
}
