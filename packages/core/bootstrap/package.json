{
  "name": "@chainlink/ea-bootstrap",
  "version": "0.1.7",
  "description": "Bootstrap an external adapter with this package",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist && rm -f tsconfig.tsbuildinfo",
    "prepublishOnly": "yarn build && yarn test",
    "setup": "yarn clean && yarn build",
    "build": "tsc -b",
    "dev:remotedev": "node ./scripts/remotedev.js",
    "lint": "eslint --ignore-path ../../../.eslintignore . --ext .ts,.tsx",
    "lint:fix": "eslint --ignore-path ../../../.eslintignore . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "yarn run mocha --exit --timeout 0 -r ts-node/register 'test/**/*.test.ts'",
    "test:unit": "mocha --exit --timeout 0 --grep @integration --invert -r ts-node/register 'test/**/*.test.ts'",
    "test:integration": "mocha --exit --timeout 0 --grep @integration -r ts-node/register 'test/**/*.test.ts'"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^4.0.1",
    "@reduxjs/toolkit": "^1.5.0",
    "axios": "^0.21.1",
    "decimal.js": "^10.2.1",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0",
    "object-hash": "^2.0.3",
    "prom-client": "^13.1.0",
    "promise-timeout": "^1.3.0",
    "redis": "^3.0.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.2.0",
    "remote-redux-devtools": "^0.5.16",
    "rxjs": "^6.6.3",
    "uuid": "^8.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/express": "^4.17.11",
    "@types/lodash": "^4.14.168",
    "@types/lru-cache": "^5.1.0",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.35",
    "@types/object-hash": "^1.3.4",
    "@types/promise-timeout": "^1.3.0",
    "@types/redis": "^2.8.28",
    "@types/redux-logger": "^3.0.8",
    "@types/remote-redux-devtools": "^0.5.4",
    "@types/sinon": "^9.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "chai": "^4.3.4",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "mocha": "^8.3.2",
    "remotedev-server": "^0.3.1",
    "sinon": "^10.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
