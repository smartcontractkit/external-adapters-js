{
  "$id": "https://external-adapters.chainlinklabs.com/schemas/oanda-adapter.json",
  "title": "Oanda External Adapter",
  "description": "External Adapter for the Oanda Data Provider (https://developer.oanda.com/).",
  "required": ["API_KEY", "API_ACCOUNT_ID", "SSE_API_KEY"],
  "type": "object",
  "properties": {
    "API_KEY": {
      "type": "string",
      "description": "API key for REST endpoint"
    },
    "API_ACCOUNT_ID": {
      "type": "string",
      "description": "API Account ID"
    },
    "API_ENPDOINT": {
      "type": "string",
      "description": "Endpoint for REST prices",
      "default": "https://exchange-rates-api.oanda.com/v2"
    },
    "INSTRUMENTS_API_ENDPOINT": {
      "type": "string",
      "description": "Endpoint for REST instruments list",
      "default": "https://api-fxtrade.oanda.com/v3"
    },
    "SSE_API_KEY": {
      "type": "string",
      "description": "API key for SSE endpoint"
    },
    "SSE_API_ENDPOINT": {
      "type": "string",
      "description": "Endpoint for SSE streaming prices",
      "default": "https://stream-fxtrade.oanda.com/v3"
    }
  },
  "allOf": [
    {
      "$ref": "https://external-adapters.chainlinklabs.com/schemas/ea-bootstrap.json"
    }
  ]
}
