{
  "openapi": "3.0.0",
  "info": {
    "title": "@chainlink/coingecko-adapter",
    "version": "0.0.3",
    "license": {
      "name": "MIT"
    }
  },
  "paths": {
    "/": {
      "post": {
        "requestBody": {
          "description": "request body for EA",
          "schema": {
            "properties": {
              "endpoint": {
                "type": "string",
                "default": "price",
                "enum": ["price", "globalmarketcap", "dominance", "marketcap"]
              }
            },
            "required": {
              "oneOf": [
                {
                  "$ref": "#/endpoints/price"
                },
                {
                  "$ref": "#/endpoints/globalmarketcap"
                },
                {
                  "$ref": "#/endpoints/dominance"
                },
                {
                  "$ref": "#/endpoints/marketcap"
                }
              ]
            }
          }
        }
      }
    },
    "endpoints": {
      "price": {
        "properties": ["coinid", "base", "from", "coin", "quote", "to", "market"],
        "required": [
          {
            "oneOf": [
              "coinid",
              {
                "oneOf": ["base", "from", "coin"]
              }
            ]
          },
          {
            "oneOf": ["quote", "to", "market"]
          }
        ]
      },
      "globalmarketcap": {
        "properties": ["market", "to", "quote", "coin"],
        "required": [
          {
            "oneOf": ["market", "to", "quote", "coin"]
          }
        ]
      },
      "dominance": {
        "properties": ["market", "to", "quote", "coin"],
        "required": [
          {
            "oneOf": ["market", "to", "quote", "coin"]
          }
        ]
      }
    }
  },
  "securityDefinitions": {
    "environment-variables": {
      "API_KEY": {
        "required": "true"
      }
    }
  },
  "components": {},
  "tags": []
}
