{
  "$id": "https://external-adapters.chainlinklabs.com/schemas/coinapi-adapter.json",
  "title": "Chainlink CoinApi External Adapter",
  "required": ["API_KEY"],
  "type": "object",
  "properties": {
    "API_ENDPOINT": {
      "type": "string",
      "default": "https://rest.coinapi.io/v1/"
    },
    "API_KEY": {
      "type": "string",
      "description": "An API key that can be obtained from  [here](https://www.coinapi.io/pricing)"
    },
    "WS_API_ENDPOINT": {
      "type": "string",
      "default": "wss://ws.coinapi.io/v1/"
    }
  },
  "allOf": [
    {
      "$ref": "https://external-adapters.chainlinklabs.com/schemas/ea-bootstrap.json"
    }
  ]
}
