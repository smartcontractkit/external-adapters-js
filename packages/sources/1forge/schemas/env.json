{
  "$id": "https://external-adapters.chainlinklabs.com/schemas/1forge-adapter.json",
  "title": "Chainlink External Adapter for 1Forge",
  "required": ["API_KEY"],
  "type": "object",
  "properties": {
    "API_KEY": {
      "type": "string",
      "description": "An API key that can be obtained from the 1Forge dashboard"
    },
    "API_ENDPOINT": {
      "type": "string",
      "default": "https://api.1forge.com/"
    }
  },
  "allOf": [
    {
      "$ref": "https://external-adapters.chainlinklabs.com/schemas/ea-bootstrap.json"
    }
  ]
}
