{
  "$id": "https://external-adapters.chainlinklabs.com/schemas/dxfeed-adapter.json",
  "title": "Chainlink External Adapter for dxFeed",
  "required": ["API_USERNAME", "API_PASSWORD"],
  "type": "object",
  "properties": {
    "API_USERNAME": {
      "type": "string"
    },
    "API_PASSWORD": {
      "type": "string"
    },
    "API_ENDPOINT": {
      "default": "https://tools.dxfeed.com/webservice/rest",
      "description": "The endpoint for your dxFeed",
      "type": "string"
    }
  },
  "allOf": [
    {
      "$ref": "https://external-adapters.chainlinklabs.com/schemas/ea-bootstrap.json"
    }
  ]
}
