{
  "$id": "https://external-adapters.chainlinklabs.com/schemas/amberdata-adapter.json",
  "title": "Chainlink External Adapter for Amberdata",
  "required": ["API_KEY"],
  "type": "object",
  "properties": {
    "API_ENDPOINT": {
      "type": "string",
      "default": "https://web3api.io"
    },
    "API_KEY": {
      "type": "string",
      "description": "API key to use"
    },
    "WS_API_ENDPOINT": {
      "type": "string",
      "default": "wss://ws.web3api.io"
    }
  },
  "allOf": [
    {
      "$ref": "https://external-adapters.chainlinklabs.com/schemas/ea-bootstrap.json"
    }
  ]
}
