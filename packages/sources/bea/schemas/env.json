{
  "$id": "https://external-adapters.chainlinklabs.com/schemas/bea-adapter.json",
  "title": "Chainlink External Adapter for the Bureau of Economic Analysis (BEA)",
  "required": ["API_KEY"],
  "type": "object",
  "properties": {
    "API_ENDPOINT": {
      "type": "string",
      "default": "https://apps.bea.gov/api"
    },
    "API_KEY": {
      "type": "string",
      "description": "An API key that can be obtained from the data provider's dashboard"
    }
  },
  "allOf": [
    {
      "$ref": "https://external-adapters.chainlinklabs.com/schemas/ea-bootstrap.json"
    }
  ]
}
