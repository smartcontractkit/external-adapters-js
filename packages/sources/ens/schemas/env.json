{
  "$id": "https://external-adapters.chainlinklabs.com/schemas/ens-adapter.json",
  "title": "ENS Adapter",
  "description": "ENS Adapter",
  "required": ["RPC_URL"],
  "type": "object",
  "properties": {
    "RPC_URL": {
      "type": "string",
      "description": "A valid Ethereum RPC URL"
    },
    "CHAIN_ID": {
      "required": false,
      "description": "The chain id to connect to",
      "type": "string",
      "default": "1"
    }
  },
  "allOf": [
    {
      "$ref": "https://external-adapters.chainlinklabs.com/schemas/ea-bootstrap.json"
    }
  ]
}
