{
  "$id": "https://external-adapters.chainlinklabs.com/schemas/currencylayer-adapter.json",
  "title": "Chainlink External Adapter for CurrencyLayer",
  "required": ["API_KEY"],
  "type": "object",
  "properties": {
    "API_KEY": {
      "type": "string",
      "description": "An API key that can be obtained from [here](https://currencylayer.com/product)",
      "required": true
    },
    "API_ENDPOINT": {
      "type": "string",
      "default": "https://api.currencylayer.com"
    }
  },
  "allOf": [
    {
      "$ref": "https://external-adapters.chainlinklabs.com/schemas/ea-bootstrap.json"
    }
  ]
}
