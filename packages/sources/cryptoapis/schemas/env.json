{
  "$id": "https://external-adapters.chainlinklabs.com/schemas/cryptoapis-adapter.json",
  "title": "Chainlink External Adapter for CryptoAPIs",
  "required": ["API_KEY"],
  "type": "object",
  "properties": {
    "API_KEY": {
      "type": "string",
      "description": "An API key that can be obtained from [here](https://cryptoapis.io/)",
      "required": true
    },
    "API_TIMEOUT": {
      "type": "number",
      "description": " Timeout parameter",
      "required": false,
      "default": "30000"
    }
  },
  "allOf": [
    {
      "$ref": "https://external-adapters.chainlinklabs.com/schemas/ea-bootstrap.json"
    }
  ]
}
