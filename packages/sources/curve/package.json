    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@chainlink/types": "0.0.1",
    "@types/jest": "26.0.24",
    "@types/node": "14.17.7",
    "@types/supertest": "2.0.11",
    "nock": "13.1.2",
    "supertest": "6.1.6",
    "typescript": "4.3.5"
  }
