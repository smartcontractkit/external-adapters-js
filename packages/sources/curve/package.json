{
  "name": "@chainlink/curve-adapter",
  "version": "0.0.1",
  "description": "Chainlink curve adapter.",
  "keywords": [
    "Chainlink",
    "LINK",
    "blockchain",
    "oracle",
    "curve"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
