    "@harmony-js/utils": "^0.1.56",
    "ethers": "5.4.7",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@chainlink/types": "workspace:*",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.19",
    "typescript": "4.3.5"
  }
