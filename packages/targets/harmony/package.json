{
  "name": "@chainlink/harmony-adapter",
  "version": "1.0.26",
  "description": "Chainlink harmony adapter.",
  "keywords": [
    "Chainlink",
    "LINK",
    "blockchain",
    "oracle",
    "harmony"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
